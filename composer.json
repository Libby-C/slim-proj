{
    "require": {
        "illuminate/database": "~5.1",
        "monolog/monolog": "^2.2",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.3",
        "slim/slim": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
